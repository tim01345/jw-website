# JourneyWell Website Environment Variables
# Copy this file to .env.local and fill in your values

# Deployment
NEXT_PUBLIC_SITE_URL=https://your-domain.vercel.app
NEXT_PUBLIC_VERCEL_URL=$VERCEL_URL

# Analytics (optional)
NEXT_PUBLIC_GA_ID=
NEXT_PUBLIC_GTM_ID=

# Contact Form (future)
NEXT_PUBLIC_CONTACT_EMAIL=team@journeywell.io

# CMS (future)
CONTENTFUL_SPACE_ID=
CONTENTFUL_ACCESS_TOKEN=

# Development
NODE_ENV=production 